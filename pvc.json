      "kind": "PersistentVolumeClaim",
      "apiVersion": "v1",
      "metadata": {
          "name": "${PVC_NAME}",
          "annotations": {
              "volume.beta.kubernetes.io/storage-class": "${STORAGE_CLASS}"
          }
      },
      "spec": {
          "accessModes": [
              "${ACCESS_MODES}"
          ],
          "resources": {
              "requests": {
                  "storage": "${STORAGE_SIZE}"
              }
         }
    }
